.page-popup {
  position: fixed;
  left: 30rpx;
  bottom: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.page-popup .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 150rpx;
  height: 180rpx;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
}
.page-popup .item > text {
  margin-top: 15rpx;
}
.page-popup .item > .set {
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  background: #d7d94a;
  line-height: 105rpx;
  font-size: 50rpx;
}
.page-popup .item > .icon-close {
  font-size: 66rpx;
  margin-top: 60rpx;
}

.picture-library-popup .library-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 15rpx ;
  height: 80rpx;
  background: #1274e7;
  line-height: 80rpx;
  color: #fff;
}
.picture-library-popup .library-box {
  padding: 10rpx;
}
.picture-library-popup .library-box .item {
  float: left;
  width: 47%;
  height: 400rpx;
  border: solid 2px rgba(0, 0, 0, 0.05);
  padding: 40rpx 0;
  background: #fff;
  margin: 10rpx 5rpx;
}
.picture-library-popup .library-box .item image {
  width: 100%;
  height: 100%;
}
.picture-library-popup .library-box .active {
  border: solid 2px #1274e7;
}
.picture-library-popup .library-btn {
  padding: 10rpx 20rpx;
  margin-top: 30rpx;
  margin-top: 15rpx;
}
.picture-library-popup .library-btn button {
  width: 180rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: inline-block;
  color: #fff;
  border: none;
  outline: none;
  font-size: 32rpx;
}
.picture-library-popup .library-btn .upload {
  width: 200rpx;
  background: #00c853;
}
.picture-library-popup .library-btn ._ok,
.picture-library-popup .library-btn .cancel {
  float: right;
}
.picture-library-popup .library-btn ._ok {
  background: #1274e7;
}
.picture-library-popup .library-btn .cancel {
  background: #e1e1e1;
  margin-right: 15rpx;
}

page,
page {
  background: #494a4b;
}
.poster-box .page-setting {
  position: fixed;
  bottom: 60rpx;
  left: 5%;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: #ffffff;
  text-align: center;
  line-height: 88rpx;
  font-size: 66rpx;
  color: #5e5e5e;
}
.poster-box .page-setting > view:nth-child(1) {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  width: 80rpx;
  height: 80rpx;
  border: solid 6rpx black;
  border-top-color: #e40f2c;
  border-right-color: #fad461;
  border-bottom-color: #2cbded;
  border-left-color: #2cbded;
  border-radius: 50%;
}
.poster-box .page-complete {
  position: fixed;
  bottom: 70rpx;
  right: 5%;
  width: 120rpx;
  height: 60rpx;
  background: #2cbded;
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #fff;
}

